#
	if(!is.null(n_gapsx)){#
		x_new<-convert2gaps(z=x,ranges_z=ranges_x,gap_width_z=gap_width_x)#
		ranges_x_new<-convert2gaps(z=ranges_x,ranges_z=ranges_x,gap_width_z=gap_width_x)#
